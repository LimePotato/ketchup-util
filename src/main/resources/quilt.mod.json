{
	"schema_version": 1,
	"quilt_loader": {
		"group": "xyz.limepot",
		"id": "ketchup",
		"version": "${version}",
		"metadata": {
			"name": "Ketchup Util",
			"description": "A small development utility mod",
			"contributors": {
				"LimePotato": "Developer"
			},
			"contact": {
				"homepage": "https://limepot.xyz",
				"issues": "https://github.com/LimePotato/ketchup-util/issues",
